{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tal-runner",
  "author": "Cy Brown",
  "module": "dist/tal-runner.esm.js",
  "size-limit": [
    {
      "path": "dist/tal-runner.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tal-runner.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "husky": "^8.0.3",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@types/react": "^18.0.29",
    "find-free-port": "^2.0.0",
    "tal-eval": "*",
    "tal-parser": "*",
    "tal-stdlib": "*"
  }
}
