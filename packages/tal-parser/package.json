{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "compile-tal-parser": "peggy -o src/parser.js src/parser.pegjs",
    "postcompile-tal-parser": "node ./scripts/fix-parser.js",
    "postinstall": "npm run compile-tal-parser",
    "prebuild": "npm run compile-tal-parser"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "tal-parser",
  "module": "dist/tal-parser.esm.js",
  "size-limit": [
    {
      "path": "dist/tal-parser.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/tal-parser.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.4",
    "husky": "^8.0.3",
    "peggy": "^4.2.0",
    "size-limit": "^8.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3"
  },
  "description": "Congrats! You just saved yourself hours of work by bootstrapping this project with TSDX. Let’s get you oriented with what’s here and how to use it.",
  "directories": {
    "test": "test"
  }
}
